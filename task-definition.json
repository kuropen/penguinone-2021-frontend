{
  "family": "penguinone-2021-frontend",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "penguinone-2021-frontend",
      "image": "kuropen/penguinone-2021-frontend",
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "ecsTaskExecutionRole",
  "cpu": "256",
  "memory": "512"
}